-			std::string err = EVENT_WRITE_FAILED + " The string passed in (\"" + data + "\") contains an invalid JSON type.";
-			m_handleWriteFailed(m_pArg, err);
-			return;
-		}
-	}
-
-	m_interface.write(output.str());

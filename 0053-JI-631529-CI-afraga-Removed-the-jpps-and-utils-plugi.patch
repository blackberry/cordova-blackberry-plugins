-void PPSInterface::sync()
-{
-	if (m_fd >= 0)
-		::fsync(m_fd);
-}
-
-void PPSInterface::close()
-{
-	if (m_fd >= 0) {
-
-		::close(m_fd);
-		m_fd = -1;
-		m_cachedRead = ppsObject();
-		m_oflags = 0;
-
-		sendEvent(PPSEvent(PPSEvent::PPS_EVENT_CLOSED));
-	}

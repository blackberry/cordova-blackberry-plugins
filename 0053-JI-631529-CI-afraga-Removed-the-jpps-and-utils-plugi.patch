-
-		if (nPos != std::string::npos) {
-			pathOptions = fullpath.substr(nPos);
-		}
-
-		if ((oflag & O_ACCMODE) != O_WRONLY) {
-
-			// This is used as the return object for the joinNotifyGroup() call
